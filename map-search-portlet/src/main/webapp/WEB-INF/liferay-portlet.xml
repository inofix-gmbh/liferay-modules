<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">
<!-- 
    liferay-portlet.xml: Main configuration file of the 
    map search portlet.
    
    Created:    2016-07-21 22:51 by Christian Berndt
    Modified:   2016-07-23 14:48 by Christian Berndt
    Version:    1.0.1
 -->
<liferay-portlet-app>
	<portlet>
		<portlet-name>map-search-portlet</portlet-name>
		<icon>/icon.png</icon>
        <configuration-action-class>ch.inofix.portlet.mapsearch.action.ConfigurationActionImpl</configuration-action-class>        
        <header-portlet-css>/css/leaflet.css</header-portlet-css>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <header-portlet-css>/css/MarkerCluster.css</header-portlet-css>
        <header-portlet-css>/css/MarkerCluster.Default.css</header-portlet-css>
        <header-portlet-javascript>/js/leaflet.js</header-portlet-javascript>
        <header-portlet-javascript>/js/leaflet.markercluster.js</header-portlet-javascript>
        <header-portlet-javascript>/js/realworld.388.js</header-portlet-javascript>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>